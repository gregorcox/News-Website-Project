<main> <h1>$journalist.getName()</h1>
    <img src="$journalist.getPicture()" alt="">
    <p id="twitter">$journalist.getTwitter()</p>
    <p>$journalist.getBio()</p></main>
#foreach($article in $articles)
<article><img src="$article.getImage()" alt=""/>
    <h2>$article.getTitle()</h2>
    <p>$article.getDate()</p>
    <p>$article.getSummary()</p>
    <p><a href="/articles/$article.getId()">Read More</a></p></article>
#end